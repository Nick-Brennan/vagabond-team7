<br><br><div class="container">
<div class="grid3">
	<h3>Sign in</h3>
	 <div class="form-sec">
	     <form>
	     <%= form_for @user, url: '/sessions', method: "post" do |f| %>
	     <%= f.text_field :email, :class => "mail", placeholder: "Email" %>
	     <%= f.password_field :password, :class => "lock", placeholder: "Password" %>
			 <div class="bottom">
				  <div class="submit">
				  <%= f.submit "Sign In"%>		
				  <%end%>		  
				  </div>
				 <div class="clear"></div>
			  </div>
		 	</form>						  
		 </div>				 
 </div>
 </div>